MessageBox {
    margin: 2 0;
    padding: 1 2;
    border: none;
    background: transparent;
    height: auto;
    min-height: 1;
    width: 100%;
}

.analysis-message {
    border: none;
    background: transparent;
    border-title-color: $text-disabled;
}

.analysis-message.streaming {
    border: none;
    background: transparent;
    border-title-color: $text;
}